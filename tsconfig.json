{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@cmp/*": ["src/components/*"],
            "@ass/*": ["src/assets/*"],
            "@lay/*": ["src/layouts/*"],
            "@data/*": ["src/data/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "strictNullChecks": true,
        "allowJs": true
    }
}
