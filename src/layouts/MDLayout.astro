---
import Layout from "./PageLayout.astro";
const { frontmatter: f } = Astro.props;
const fixedDate = new Date(f.lastUpdated)
---

<Layout
    pageTitle={f.pageTitle}
    description={f.description}
    back={f.back}
    icon={f.icon}
    uc={f.uc}
    lastUpdated={fixedDate}
>
    <slot />
</Layout>