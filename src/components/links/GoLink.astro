---
import Icon from "@cmp/Icon.astro";

const { go, displayText, icon } = Astro.props;
interface Props {
    go: string
    displayText: string
    icon?: string
}
---

<ul class="fa-ul">
    <li>
        <Icon override={icon} fallback="fa-solid fa-arrow-right" li={true} />
        <a href={go}> {displayText}</a>
    </li>
</ul>
